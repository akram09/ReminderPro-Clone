            android:elevation="15dp"
            android:id="@+id/categorie_fragment_setting_bluecategory_checkbox"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="2dp"
        android:backgroundTint="#28292C"
        android:background="?android:attr/listDivider"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:layout_marginBottom="3dp"
        android:layout_marginLeft="6dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_star_green_24dp"
            android:elevation="12dp"
            android:layout_marginTop="6dp"/>
        <Button
            android:layout_width="match_parent"
            android:background="@color/colorbackgroundbluewhite"
            android:layout_height="wrap_content"
            android:id="@+id/categorie_fragment_setting_greencategory_btn"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Category-Green"
            android:textColor="#28292C"
            android:textSize="16dp"
            android:layout_marginLeft="28dp"
            android:layout_marginTop="9dp"
            android:elevation="15dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enabled"
            android:textSize="15dp"
