<?xml version="1.0" encoding="utf-8"?>
<layout>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorbackgroundbluewhite"
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:layout_marginTop="12dp"
        android:layout_marginLeft="6dp">
        <Button
            android:layout_width="match_parent"
            android:background="@color/colorbackgroundbluewhite"
            android:layout_height="wrap_content"
            android:id="@+id/categorie_fragment_setting_nocategory_btn"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="No Category"
            android:textColor="#28292C"
            android:textSize="16dp"
           android:layout_marginLeft="28dp"
            android:layout_marginTop="9dp"
            android:elevation="15dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enabled"
            android:textSize="15dp"
            android:id="@+id/categorie_fragment_setting_nocategory_textview"
            android:layout_alignBottom="@id/categorie_fragment_setting_nocategory_btn"
            android:elevation="15dp"
            android:textColor="#8F93A1"
            android:layout_marginLeft="28dp"
            />
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@id/categorie_fragment_setting_nocategory_btn"
            android:elevation="15dp"
            android:id="@+id/categorie_fragment_setting_nocategory_checkbox"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="2dp"
        android:backgroundTint="#28292C"
        android:background="?android:attr/listDivider"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:layout_marginBottom="3dp"
        android:layout_marginLeft="6dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_star_red_24dp"
            android:elevation="12dp"
            android:layout_marginTop="6dp"/>
        <Button
            android:layout_width="match_parent"
            android:background="@color/colorbackgroundbluewhite"
            android:layout_height="wrap_content"
            android:id="@+id/categorie_fragment_setting_redcategory_btn"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Category-Red"
            android:textColor="#28292C"
            android:textSize="16dp"
            android:layout_marginLeft="28dp"
            android:layout_marginTop="9dp"
            android:elevation="15dp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Enabled"
            android:textSize="15dp"
            android:id="@+id/categorie_fragment_setting_categoryred_textview"
            android:layout_alignBottom="@id/categorie_fragment_setting_redcategory_btn"
            android:elevation="15dp"
            android:textColor="#8F93A1"
            android:layout_marginLeft="28dp"
            />
        <CheckBox
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@id/categorie_fragment_setting_redcategory_btn"
            android:elevation="15dp"
            android:id="@+id/categorie_fragment_setting_redcategory_checkbox"
            />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginBottom="2dp"
        android:backgroundTint="#28292C"
        android:background="?android:attr/listDivider"
        />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="0"
        android:layout_marginBottom="3dp"
        android:layout_marginLeft="6dp">
        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_star_blue_24dp"
            android:elevation="12dp"
            android:layout_marginTop="6dp"/>
        <Button
            android:layout_width="match_parent"
            android:background="@color/colorbackgroundbluewhite"
